# Copy this to .env and fill in your values

# Database
DATABASE_URL="file:./data/registrations.db"

# JWT Secret (generate a secure random string)
JWT_SECRET="your-super-secret-jwt-key-here"

# Admin password hash (use scripts/hash-password.js to generate)
# Example: node scripts/hash-password.js yourSecurePassword123
ADMIN_PASSWORD_HASH="$2b$12$your-bcrypt-hashed-password-here"